.search {
  height: 100vh;
  background-color: #fff;
}

.s-input {
  margin-top: 1vh;
  margin-left: 10px;
  margin-right: 10px;
}

.van-search__content {
  border: 1px solid #2c2c2c;
  background-color: #fff !important;
  color: #2c2c2c !important;
}

.container {
  margin-top: 15px;
}

.list {
  display: flex;
  height: 10vh;
  background-color: #2c2c2c;
  margin: 0 20px 20px 20px;
  border-radius: 15px;
  justify-content: space-between;
  align-items: center;
  color: #fff;
}

.none {
  width: 300px;
  height: 300px;
  margin: 0 auto;
}

.list image {
  margin-right: 20px;
  border-radius: 50%;
}

.list view {
  margin-left: 10px;
}

.title {
  font-size: 18px;
  margin-bottom: 10px;
  font-weight: 500;
}

.description {
  font-size: 14px;
}

.scroll-to-top {
  position: fixed;
  bottom: 60px;
  right: 30px;
  width: 50px;
  height: 50px;
  background-color: #2c2c2c;
  color: white;
  border-radius: 50%;
  display: flex;
  justify-content: center;
  align-items: center;
  z-index: 100;
  box-shadow: 0 0 10px rgba(0,0,0,0.2);
}
